<zk>
	<label>Simplified Syntax</label>
	<div viewModel="@id('vm') @init('org.zkoss.zktest.test2.F90_ZK_MVVM', type='syntax')" style="overflow:auto" width="500px" height="500px">
		<div>
			<label value="@(selItem)"/>
			<!-- mix use exception -->
			<!-- <button onClick="@('doClick', 123, bb='aas,sa2')"/> -->
			<button onClick="@('doTest', 'aa', 12, 2)"/>
		</div>
		<separator />
