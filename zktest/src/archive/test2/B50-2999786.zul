<zk>
<html>
<![CDATA[
<ol>
<li>If the bottom side of the content is not chopped half way, it is OK</li>
</ol>
]]>
</html>
<window hflex="min" vflex="min" title="my window">
	<borderlayout hflex="min" vflex="min">
		<west hflex="min" vflex="min">
			<grid hflex="min" vflex="min">
			<rows>
			<row>
				<label value="File:"/>
				<textbox width="98%"/>
			</row>
			<row>
				<label value="Type:"/>
				<hbox>
					<listbox rows="1" mold="select">
						<listitem label="Java Files,(*.java)"/>
						<listitem label="All Files,(*.*)"/>
					</listbox>
					<button label="Browse..."/>
				</hbox>
			</row>
			<row>
				<label value="Options:"/>
				<textbox rows="3" hflex="min" vflex="min"/>
			</row>
			</rows>
			</grid>
		</west>
		<east hflex="min" vflex="min">
			<grid hflex="min" vflex="min">
			 	<rows>
					<row>
						<label value="Top East"/>
						<textbox />
					</row>
				</rows>
			</grid>
			
		</east>
	</borderlayout>
</window>
</zk>