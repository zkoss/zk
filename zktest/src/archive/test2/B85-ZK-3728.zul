<zk>
    <label>You should see all icons are two stack icons.</label>
    <separator />
    <style>
        .red{
        color: red;
        }
    </style>

    <button iconSclass="z-icon-camera z-icon-stack-1x ; z-icon-ban z-icon-stack-2x red"></button>
    <checkbox iconSclass="z-icon-circle z-icon-stack-2x ; z-icon-flag z-icon-stack-1x z-icon-inverse"></checkbox>
