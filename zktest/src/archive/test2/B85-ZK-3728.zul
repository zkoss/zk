    <separator height="50px"></separator>
    <tabbox>
        <tabs>
            <tab iconSclass="z-icon-circle z-icon-stack-2x ; z-icon-flag z-icon-stack-1x z-icon-inverse" ></tab>
            <tab iconSclass="z-icon-camera z-icon-stack-1x ; z-icon-ban z-icon-stack-2x red"></tab>
