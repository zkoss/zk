<zk>
<zscript><![CDATA[
import java.util.*;
ListModelList model = new ListModelList();
model.add("test");

public void add(){
	model.add(System.currentTimeMillis()+"");
}
]]></zscript>
	<listbox id="box" model="${model}" rows="5">
		<template name="model">
			<listitem label="${each}"></listitem>
		</template>
	</listbox>
	<button onClick="add()"/>
</zk>
