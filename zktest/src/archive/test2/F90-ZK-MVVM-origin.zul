<zk>
	<label>Origin MVVM</label>
	<div viewModel="@id('vm') @init('org.zkoss.zktest.test2.F90_ZK_MVVM', type='syntax')" style="overflow:auto" width="500px" height="500px">
		<div>
			<label value="@load(vm.selItem)"/>
			<button onClick="@command('doTest_Origin', aa='aa', bb=12, cc=2)"/>
		</div>
		<separator />
		<label value="@load(vm.text)"/>
		<separator />
		<div height="60px" style="overflow: auto">
			<vlayout>
				<forEach items="@load(vm.list)">
					<div>
						<label value="@load(each)"></label>
					</div>
				</forEach>
			</vlayout>
		</div>
	</div>
</zk>
