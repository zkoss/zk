            <tab label="Tab 5" closable="true"/>
        </tabs>
        <toolbar>
            <toolbarbutton label="1"/>
            <toolbarbutton label="2"/>
            <toolbarbutton label="3"/>
        </toolbar>
        <tabpanels>
            <tabpanel>
                <textbox/>
            </tabpanel>
            <tabpanel>
                <textbox/>
