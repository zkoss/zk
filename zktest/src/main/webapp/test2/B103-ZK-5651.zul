    <label multiline="true">
        1. Type characters in the textbox, then press Enter before onChange is triggered.
        2. Expected result: self.value should be the textbox value at the moment the user presses Enter.
    </label>
    <textbox id="tb" multiline="true" submitByEnter="true" instant="true"
             onOK='Clients.log("onOK: "+self.value)'
             onChange='Clients.log("onChange: "+self.value)' value="abcd" />
</zk>