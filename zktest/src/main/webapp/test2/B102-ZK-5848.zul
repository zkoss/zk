<zk>
    <label multiline="true">
        1. Below are 1 textbox and 4 readonly NumberInputElements
        2. copy the number in the textbox by keyboard
        3. press TAB and CTRL+V to paste the copied text into each NumberInputElement
        4. You shouldn't see the text paste into any NumberInputElement
    </label>
    <vlayout>
        <hlayout>
            <label>textbox</label>
            <textbox value="516"/>
        </hlayout>
        <hlayout>
            <label>intbox</label>
            <intbox readonly="true" value="918"/>
        </hlayout>
        <hlayout>
            <label>doublebox</label>
            <doublebox readonly="true" value="918"/>
        </hlayout>
        <hlayout>
            <label>longbox</label>
            <longbox readonly="true" value="918"/>
        </hlayout>
        <hlayout>
            <label>decimalbox</label>
            <decimalbox readonly="true" value="918"/>
        </hlayout>
    </vlayout>
</zk>
