<zk>
    <zscript><![CDATA[
        ListModelList model = new ListModelList(Locale.getAvailableLocales());
        ListSubModel subModelDefault = ListModels.toListSubModel(model);
    ]]></zscript>

    <label multiline="true">
        1. This is the default filter logic of Chosenbox.
        2. It uses "startsWith" logic by default, so the behavior is the same as the first Chosenbox above.
    </label>

    <label>startsWith</label>
    <chosenbox width="400px" model="${subModelDefault}">
        <custom-attributes org.zkoss.zul.chosenbox.filterLogic="startsWith"/>
    </chosenbox>
</zk>