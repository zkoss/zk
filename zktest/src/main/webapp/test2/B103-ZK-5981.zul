<zk>
    <label multiline="true">
        1. press the data-handler button on the page
        2. there should be no console errors
    </label>
    <div id="host" xmlns:ca="client/attribute">
        <button label="data-handler" onClick='Div div = new Div();div.setClientAttribute("data-foo","bar");host.appendChild(div);' />
        <button label="onbind listener" onClick='Div div = new Div();div.setWidgetListener("onBind","console.log(`did on bind`);");host.appendChild(div);' />
    </div>
</zk>