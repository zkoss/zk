			import org.zkoss.zk.au.out.AuWrongValue;                               
			if (Messagebox.show("error message") ==Messagebox.OK) {
            	AuWrongValue a = new AuWrongValue(tb, "error message");
                Clients.response(a);            		
            }
		]]></attribute>
	</button>
