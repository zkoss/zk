							See: http://www.librarything.com/talktopic.php?topic=13113
							-->
						<treeitem id="mimorebtn" label="More button features" value="/userguide/misc/moreButton.zul"/>
						<treeitem id="mi2" label="Include" value="/userguide/misc/include.zul"/>
