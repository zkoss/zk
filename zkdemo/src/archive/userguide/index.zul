	<center autoscroll="true">
		<include style="padding:3px;" width="99%" id="xcontents" src="${requestScope.contentSrc}"/><!-- prepared by TocTree -->
	</center>
</borderlayout>
<script type="text/javascript"
if="${desktop.execution.serverName == &quot;www.zkoss.org&quot; or desktop.execution.serverName == &quot;www.potix.com&quot;}"><![CDATA[
if (!zk.https()) {
	_urchin = function() {
		try {
			_uacct = "UA-121377-3";
			urchinTracker();
		} catch (e) {}
	};
	setTimeout('zk.loadJS("http://www.google-analytics.com/urchin.js", _urchin)', 5000);
}
]]></script>
</zk>
