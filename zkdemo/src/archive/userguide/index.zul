						<treeitem id="anima" label="Animations" value="/userguide/misc/anima.zul"/>
						<treeitem id="theme" label="Change Theme" value="/userguide/misc/changetheme.zul"/>
						<treeitem id="captcha" label="Captcha" value="/userguide/misc/captcha.zul"/>
						<treeitem id="mi1" label="Timer" value="/userguide/misc/timer.zul"/>
						<treeitem id="miaudio" label="Audio" value="/userguide/misc/audio.zul" unless="${c:isGecko()}"/>
