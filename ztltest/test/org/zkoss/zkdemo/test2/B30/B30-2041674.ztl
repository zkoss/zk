<test tags="B30-2041674.zul,ie9,Menuitem">
	<case id="ztl">
		<server><!--
			


<zk
	xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml">
	<window>
		<div>Hover mouse over disabled item, popup menu disappears(This is wrong.)</div>

		<menubar autodrop="true">
			<menu label="Popup Menu">
				<menupopup>
					<menuitem
						label="Hover mouse here..."
						href="http://www.zkoss.org"
						disabled="true" />
					<menuitem
						label="OK Here"
						href="http://www.zkoss.org" />
				</menupopup>
			</menu>
		</menubar>
	</window>
</zk>

		--></server>
		<client><![CDATA[
				mouseOver(jq(".z-menu-body"));waitResponse();
				mouseOver(jq(".z-menu-item:eq(0)"));sleep(500);
				verifyTrue(jq(".z-menu-popup").isVisible());
		]]></client>
	</case>
</test>