<test tags="toolbarbutton,menupopup">
	<case id="popup">
		<server><!--
			<zk>
				<toolbarbutton id="tb" popup="popup" label="Click Me without error, that is correct!(IE only)"/>
				<menupopup id="popup">
				</menupopup>
			</zk>
		--></server>
		<client><![CDATA[
			click(tb);
			waitResponse();
			verifyFalse(jq(".z-error").exists());
		]]></client>
	</case>
</test>