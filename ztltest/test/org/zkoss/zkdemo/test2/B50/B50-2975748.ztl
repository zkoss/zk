<test tags="B50-2975748.zul,button,popup">
	<case id="popup">
		<server id="zul"><![CDATA[
			<zk>
			   <button id="btn" popup="popup" onClick="" />
			   <popup id="popup"/>
			</zk>
		]]></server>
		<client><![CDATA[
				click(btn);
				waitResponse();
				verifyTrue(popup.exists());
		]]></client>
	</case>
</test>