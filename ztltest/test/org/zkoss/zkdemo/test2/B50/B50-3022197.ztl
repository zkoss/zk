<test tags="B50-3022197.zul,A,E,Tabbox">
	<case id="ztl">
		<server><!--
			<zk>
   <html><![CDATA[
	<ul>
	<li>click "set selected"</li>
	<li>select "B" tab</li>
	<li>"BB" tab shall be selected</li>
	</ul>
	]]></html>
	<button label="set selected" onClick="tabB.selected = true;" />
	<tabbox>
		<tabs>
			<tab label="A" />
			<tab label="B" id="b" />
		</tabs>
		<tabpanels>
			<tabpanel>A</tabpanel>
			<tabpanel>
				<tabbox>
					<tabs>
						<tab label="BA" />
						<tab label="BB" id="tabB"/>
					</tabs>
					<tabpanels>
						<tabpanel>BA</tabpanel>
						<tabpanel>BB</tabpanel>
					</tabpanels>
				</tabbox>
			</tabpanel>
		</tabpanels>
	</tabbox>
</zk>

		--></server>
		<client><![CDATA[
				click(jq("@button"));
				waitResponse();
				click(b);
				waitResponse();
				verifyEquals("BB",jq(".z-tab-seld:eq(1) .z-tab-text").text());
		]]></client>
	</case>
</test>