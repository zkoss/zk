<test tags="B50-2995770.zul,menu">
	<case id="ChangeIcon">
		<server id="zul"><![CDATA[
<zk>
<window title="Region Disappears" border="normal">
	<tabbox>
		<tabs>
			<tab id="t1" label="tab 1" />
			<tab id="t2" label="tab 2" />
		</tabs>
		<tabpanels>
			<tabpanel>
				<borderlayout height="500px">
					<center>
						<label value="center" />
					</center>
					<east id="east" size="200px" collapsible="true" open="false">
						<label value="east" />
					</east>
				</borderlayout>
			</tabpanel>
			<tabpanel>
				<button id="btn" label="Go" onClick="east.open = true" />
			</tabpanel>
		</tabpanels>
	</tabbox>
</window>
</zk>
		]]></server>
		<client><![CDATA[
			click(t2);
			click(btn);
			waitResponse();
			click(t1);
			waitResponse();
			verifyEquals(200, jq(east.$n("real")).width());
		]]></client>
	</case>
</test>