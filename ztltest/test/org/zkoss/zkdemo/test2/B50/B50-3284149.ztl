<test tags="B50-3284149.zul,A,E,Datebox">
	<case id="ztl">
		<server><!--
			


<zk>
	<html><![CDATA[
		<ol>
			<li>Open the datebox.</li>
			<li>Click the timebox in datebox popup.</li>
			<li>The time shall not changed.</li>
		</ol>
	]]></html>
	<datebox format="yyyy-MM-dd HH:mm:ss" readonly="true"/>
</zk>

		--></server>
		<client><![CDATA[
			click(jq("@datebox .z-datebox-btn"));
				waitResponse();
				String x = jq(".z-datebox-pp .z-timebox input").val();
				focus(jq(".z-datebox-pp .z-timebox input"));
				waitResponse();
				String x1 = jq(".z-datebox-pp .z-timebox input").val();
				verifyEquals(x,x1);
		]]></client>
	</case>
</test>