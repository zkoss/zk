<test tags="B50-2987076.zul,tree,treeitem,treerow,treecell,Unstable">
	<case id="tooltiptext">
		<server id="zul"><![CDATA[
			<zk>
				<button id="setBtn" onClick='tr.tooltiptext="row"'/>
				<button id="getBtn" onClick='msg.value = tr.tooltiptext'/>
				<label id="msg"/>
			   <tree>
					<treechildren>
						<treeitem>
							<treerow id="tr">
								<treecell>
									Item 1
								</treecell>					
							</treerow>
						</treeitem>
					</treechildren>
				</tree>
			</zk>
		]]></server>
		<client><![CDATA[
			click(setBtn);
			waitResponse();
			click(getBtn);
			waitResponse();
			verifyEquals("row", jq(msg).html());
		]]></client>
	</case>
</test>