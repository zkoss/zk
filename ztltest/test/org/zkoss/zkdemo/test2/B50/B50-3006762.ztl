<test tags="B50-3006762.zul,tree,treeitem">
	<case id="scrollHeight">
		<server id="zul"><![CDATA[
			<zk>
				<tree id="tree" multiple="true">
					<treechildren>
						<treeitem>
							<treerow>
								<treecell label="1" />
							</treerow>
							<treechildren>
								<treeitem>
									<treerow>
										<treecell label="1.1" />
									</treerow>
									<treechildren>
										<treeitem>
											<treerow>
												<treecell label="1.1.1" />
											</treerow>
										</treeitem>
										<treeitem>
											<treerow>
												<treecell label="1.1.2" />
											</treerow>
										</treeitem>
										<treeitem>
											<treerow>
												<treecell label="1.1.3" />
											</treerow>
										</treeitem>
									</treechildren>
								</treeitem>
							</treechildren>
						</treeitem>
					</treechildren>
				</tree>
			</zk>
		]]></server>
		<client><![CDATA[
			click(jq("@treecell[label=\"1.1.1\"] div.z-overflow-hidden"));
			if(ZK.is("ie"))
				blur(jq("a"));
			click(jq("@page @treerow span.z-tree-root-open"));
			Element node = tree.$n().firstChild();
			verifyEquals(node.get("clientHeight"), node.get("scrollHeight"));
		]]></client>
	</case>
</test>