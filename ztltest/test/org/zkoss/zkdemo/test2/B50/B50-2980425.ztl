<test tags="B50-2980425.zul,listbox,listfooter,grid,footer">
	<case id="Align">
		<server><!--
		<listbox width="200px">
		    <listhead>
		        <listheader label="Population"/>
		        <listheader align="right" label="%"/>
		    </listhead>
		    <listitem id="a" value="A">
		        <listcell label="A. Graduate"/>
		        <listcell label="20%"/>
		    </listitem>
		    <listitem id="b" value="B">
		        <listcell label="B. College"/>
		        <listcell label="23%"/>
		    </listitem>
		    <listitem id="c" value="C">
		        <listcell label="C. High School"/>
		        <listcell label="40%"/>
		    </listitem>
		    <listitem id="d" value="D">
		        <listcell label="D. Others"/>
		        <listcell label="17%"/>
		    </listitem>
		    <listfoot>
		        <listfooter label="More or less"/>
		        <listfooter id="listfooter" label="100%"/>
		    </listfoot>
		</listbox>
		--></server>
		<client>
			verifyEquals("right", jq(listfooter).attr("align"));
		</client>
		<server><!--
			<zk>
			<grid>
			<columns>
				<column label="column 1"/>
				<column label="column 2" align="right"/>
			</columns>
			<rows>
			<row>
			<label value="File:"/>
			<textbox width="98%"/>
			</row>
			<row id="row">
			<label value="File:"/>
			<textbox width="98%"/>
			</row>
			<row>
			<label value="Options:"/>
			<textbox rows="3" width="98%"/>
			</row>
			</rows>
				<foot>
					<footer label="footer 1"/>
					<footer id="footer" label="footer 1"/>
				</foot>
			</grid>
			</zk>
		 --></server>
		<client><![CDATA[
		     verifyEquals("right", jq(footer).attr("align"));
		]]></client>
	</case>
</test>