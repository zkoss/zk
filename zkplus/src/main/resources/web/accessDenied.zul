<window title="Access Denied" border="normal" width="500px" onOK="doOK(self)">
  <div style="text-align:center"><label value="${requestScope.ACEGI_SECURITY_403_EXCEPTION}"/></div>
  <div style="text-align:center"><button label="OK" onClick="doOK(spaceOwner)"/></div>
  <zscript>
    void doOK(Window modalwin) {
      modalwin.detach();
    }
  </zscript>
</window>
